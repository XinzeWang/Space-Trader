<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="pineapple.Controller2CharacterSheet">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Label fx:id="title" layoutX="300.0" layoutY="24.0" prefHeight="69.0" prefWidth="264.0"
                       text="Character Sheet">
                    <font>
                        <Font size="30.0"/>
                    </font>
                </Label>
                <Label layoutX="112.0" layoutY="100.0" prefHeight="23.0" prefWidth="210.0" text="Character Name :">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label layoutX="112.0" layoutY="150.0" prefHeight="23.0" prefWidth="210.0" text="Difficulty :">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label fx:id="characterName" layoutX="250.0" layoutY="100.0" prefHeight="23.0" prefWidth="210.0">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label fx:id="difficulty" layoutX="250.0" layoutY="150.0" prefHeight="23.0" prefWidth="210.0">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label layoutX="112.0" layoutY="200.0" prefHeight="23.0" prefWidth="210.0" text="Pilot Point :">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label layoutX="112.0" layoutY="250.0" prefHeight="23.0" prefWidth="210.0" text="Fighter Point :">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label layoutX="112.0" layoutY="300.0" prefHeight="23.0" prefWidth="210.0" text="Merchant Point :">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label layoutX="112.0" layoutY="350.0" prefHeight="23.0" prefWidth="210.0" text="Engineer Point :">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label layoutX="112.0" layoutY="400.0" prefHeight="23.0" prefWidth="210.0" text="Credits :">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label fx:id="pilotPoints" layoutX="250.0" layoutY="200.0" prefHeight="23.0" prefWidth="210.0">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label fx:id="fighterPoints" layoutX="250.0" layoutY="250.0" prefHeight="23.0" prefWidth="210.0">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label fx:id="merchantPoints" layoutX="250.0" layoutY="300.0" prefHeight="23.0" prefWidth="210.0">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label fx:id="engineerPoints" layoutX="250.0" layoutY="350.0" prefHeight="23.0" prefWidth="210.0">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>

                <Label fx:id="credit" layoutX="250.0" layoutY="400.0" prefHeight="23.0" prefWidth="210.0">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Button fx:id="enter" layoutX="370.0" layoutY="450.0" mnemonicParsing="false" prefHeight="47.0"
                        prefWidth="151.0" text="Enter a Region" onAction="#enterRegion"/>
                <Button fx:id="shipInfo" layoutX="220.0" layoutY="450.0" mnemonicParsing="false" prefHeight="47.0"
                        prefWidth="100.0" text="Ship Info" onAction="#showShipInfo"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
